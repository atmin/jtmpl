/*

## Variable

*/

    describe('Variable {{variable}}', function () {

        var model = {
            a: 1
        };

/*

Expect proper compilation

*/

        it('{{a}} -> 1', function () {
            result = jtmpl('{{a}}', model);
            expect(result).toBe('1');
        });


        it('<span>{{a}}</span> -> <span>1</span>', function () {
            result = jtmpl('<span>{{a}}</span>', model)
            expect(result).toBe('<span>1</span>');
        });

    }
